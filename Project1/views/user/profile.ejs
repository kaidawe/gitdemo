<h1>My Profile</h1>
  <% if(profileInfo.imagePath) { %>
    <img src="<%= profileInfo.imagePath %>" alt="<%= profileInfo.username %>" height = 300px />
  <% } %>
  <br/>

  Username: <%= profileInfo.user.username %><br/>
  Name: <%= profileInfo.user.firstName %> <%= profileInfo.user.lastName %><br/>
  Email: <%= profileInfo.user.email %><br/>
  Interests: <%= profileInfo.user.interests %><br/>
  
  <% if(reqInfo.roles?.length) { %>
  Roles: <%= reqInfo.roles %>
  <% if(reqInfo.username == profileInfo.user.username || reqInfo.roles?.length) { %>
  <a href="/user/edit/<%= reqInfo.username %>"><button>Edit</button></a>
  <% } %>
  <% } %>